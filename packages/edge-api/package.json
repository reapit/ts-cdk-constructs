{
  "name": "@reapit-cdk/edge-api",
  "version": "0.0.0",
  "homepage": "https://github.com/reapit/ts-cdk-constructs/blob/main/packages/edge-api",
  "readme": "https://github.com/reapit/ts-cdk-constructs/blob/main/packages/edge-api/readme.md",
  "bugs": {
    "url": "https://github.com/reapit/ts-cdk-constructs/issues"
  },
  "license": "MIT",
  "scripts": {
