  "devDependencies": {
    "@aws-sdk/client-dynamodb": "3.414.0",
    "@aws-sdk/client-secrets-manager": "3.414.0",
    "@aws-sdk/lib-dynamodb": "3.414.0",
    "@reapit-cdk/eslint-config": "workspace:^",
    "@reapit-cdk/tsup": "workspace:^",
    "@reapit-cdk/version-package": "workspace:^",
